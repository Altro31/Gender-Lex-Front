{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "react": ["node_modules/preact/compat/"],
            "react-dom": ["node_modules/preact/compat/"],
            "~types/*": ["src/lib/types/*"],
            "~ui/*": ["src/components/ui/*"],
            "@/*": ["./src/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "preact"
    },
    "exclude": ["node_modules", "dist", "src/components/ui"]
}
