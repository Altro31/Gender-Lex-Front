---
import { Image } from "astro:assets"

interface Props {
    src: any
    alt: string
    width?: number
    height?: number
}
const { height = 40, width = 40, src, alt } = Astro.props
---

<Image
    src={src}
    alt={alt}
    height={height}
    width={width}
    class="rounded-full cursor-pointer"
/>
