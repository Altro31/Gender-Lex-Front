{
    "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-astro"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ],
    "semi": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "vueIndentScriptAndStyle": true,
    "endOfLine": "lf"
}
